{
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "camelcase": 2,
    "consistent-return": 0,
    "curly": [2, "all"],
    "eol-last": [2, "unix"],
    "eqeqeq": [2, "allow-null"],
    "guard-for-in": 2,
    "indent": [2, 2, {"SwitchCase": 1, "VariableDeclarator": 2}],
    "no-alert": 1,
    "no-comma-dangle": 0,
    "no-console": 1,
    "no-debugger": 1,
    "no-global-strict": 0,
    "no-underscore-dangle": 0,
    "no-unused-vars": ["error", { "varsIgnorePattern": "React" }],
    "no-use-before-define": 1,
    "semi": 1,
    "quotes": [2, "single"],
    "wrap-iife": [2, "inside"],
    "react/jsx-uses-react": "error",
    "react/jsx-uses-vars": "error"
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true,
    }
  },
  "globals": {
    "afterEach": true,
    "angular": true,
    "beforeEach": true,
    "describe": true,
    "expect": true,
    "inject": true,
    "it": true,
    "module": true,
    "spyOn": true
  },
  "plugins": [
    "react"
  ]
}
