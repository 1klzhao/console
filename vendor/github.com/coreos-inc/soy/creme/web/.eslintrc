{
  "env": {
    "browser": true,
  },
  "rules": {
    "block-scoped-var": 1,
    "camelcase": 2,
    "comma-dangle": 0,
    "consistent-return": 0,
    "curly": [2, "all"],
    "dot-notation": 1,
    "eqeqeq": 2,
    "guard-for-in": 2,
    "max-len": [1, 120],
    "no-alert":1,
    "no-comma-dangle": 0,
    "no-console":1,
    "no-debugger":1,
    "no-else-return": 2,
    "no-eq-null": 2,
    "no-extend-native": 2,
    "no-extra-semi": 2,
    "no-fallthrough": 2,
    "no-global-strict": 0,
    "no-loop-func": 2,
    "no-multi-spaces": 2,
    "no-native-reassign": 2,
    "no-new-wrappers": 2,
    "no-param-reassign": 2,
    "no-redeclare": 2,
    "no-return-assign": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,
    "no-with": 2,
    "quotes": [2, "single"],
    "semi": 2,
    "space-infix-ops": 2,
    "vars-on-top": 2,
    "wrap-iife": [2, "outside"],
    "yoda": [2, "never"],
    "strict": [2, "function"]
  },
  "globals": {
    "angular": true,
    "inject": true,
    "module": true,
    "describe": true,
    "spyOn": true,
    "beforeEach": true,
    "afterEach": true,
    "expect": true,
    "it": true
  }
}
